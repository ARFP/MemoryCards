<UserControl x:Class="MemoryCards.UserControlAllEtalonnage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MemoryCards"
             mc:Ignorable="d">
    <Grid x:Name="gridUserControl" Height="19">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition Width="70"/>
            <ColumnDefinition Width="70"/>
        </Grid.ColumnDefinitions>
        <TextBlock Text="{Binding Path=AgeRange}" Grid.Column="0" HorizontalAlignment="Center"/>
        <TextBlock Text="{Binding Path=NumberPersoneTest}" Grid.Column="1" HorizontalAlignment="Center"/>
        <TextBlock Text="{Binding Path=AverageAge}" Grid.Column="2" HorizontalAlignment="Center"/>
        <TextBlock Text="{Binding Path=SDAge}" Grid.Column="3" HorizontalAlignment="Center"/>
    </Grid>
</UserControl>
